    "import jax\n",
    "import jax.numpy as jnp\n",
    "\n",
    "def self_attention(X, Q, K, V, beta):\n",
    "    XQ = X @ Q.T\n",
    "    XK = X @ K.T\n",
    "    inners = XQ @ XK.T\n",
    "    scores = jax.nn.softmax(inners * beta, axis=1)\n",
    "    return scores @ (X @ V.T)\n"
   ]
